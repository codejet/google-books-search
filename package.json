{
  "name": "google-books-search",
  "author": "Oliver Schmidt",
  "contributors": [
    {
      "name": "Rafal Bromirski",
      "url": "https://github.com/paranoida"
    }
  ],
  "description": "A reactive custom element for searching Google Books",
  "license": "MIT",
  "main": "dist/build/build.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/codejet/google-books-search/tree/reactive-element"
  },
  "keywords": [
    "web-component",
    "web-components",
    "custom-element",
    "custom-elements",
    "react",
    "reactive-elements",
    "google-books"
  ],
  "scripts": {
    "start": "webpack-dev-server",
    "test": "jest",
    "watch": "webpack -w"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ],
    "moduleNameMapper": {
      "../css/[A-z_-]+(?!-css).css$": "__mocks__/css.js"
    }
  },
  "dependencies": {
    "document-register-element": "^0.4.3",
    "qwest": "^1.7.0",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-css-modules": "^3.7.6",
    "react-dom": "^0.14.7",
    "reactive-elements": "^0.6.3"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-jest": "^11.0.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.15.0",
    "jest-cli": "^11.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.1"
  }
}
